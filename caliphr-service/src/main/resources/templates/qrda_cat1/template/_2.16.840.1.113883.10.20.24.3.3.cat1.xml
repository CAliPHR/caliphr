<entry>
    <act classCode="ACT" moodCode="EVN" th:attr="negationInd=${entry.negationInd}">
        <templateId root="2.16.840.1.113883.10.20.24.3.3"/>
        <id root="1.3.6.1.4.1.115" th:attr="extension=${entry.id}"/>
        <code th:attr="code=${entry.code}, codeSystem=${entry.codeSystem}, 'sdtc:valueSet'=${entry.codeValueSetOid}">
            <originalText th:text="${entry.codeDescription}"></originalText>
        </code>
        <text th:text="${entry.description}"/>
        <statusCode code="completed"/>

        <effectiveTime>
            <low th:attr="value=${entry.low}" />
            <high th:attr="value=${entry.high}" />
        </effectiveTime>

        <participant typeCode="AUT">
            <participantRole classCode="ASSIGNED">
                <code code="158965000" codeSystem="2.16.840.1.113883.6.96" codeSystemName="SNOMED CT"
                      displayName="Medical Practitioner"/>
            </participantRole>
        </participant>

        <participant typeCode="IRCP">
            <participantRole classCode="PAT"/>
        </participant>
        <th:block th:include="qrda_cat1/_reason.cat1.xml" th:with="reason=${entry.reason}"
                  th:if="${entry.reason != null}"/>
    </act>
</entry>